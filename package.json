{
  "name": "react-mosaic-component",
  "version": "6.1.0",
  "description": "A React Tiling Window Manager",
  "license": "Apache-2.0",
  "type": "commonjs",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config webpack/hot.ts --port 3001",
    "json-server": "json-server ./db/db.json",
    "build": "npm-run-all -lp build:**",
    "build:less": "lessc --autoprefix src/app/styles/index.less src/app/styles/react-mosaic-component.css",
    "bundle": "webpack --config webpack/bundle.ts",
    "start": "concurrently \"npm run dev\" \"npm run json-server\" \"npm run tailwind\"",
    "start:prod": "npm-run-all build bundle && json-server ./db/db.json",
    "prettier:run": "prettier 'styles/*.less' '*.md' '{,.}*.yml' '.circleci/*.yml'",
    "fix": "npm-run-all -lp fix:**",
    "fix:format": "yarn run prettier:run --write",
    "fix:lint": "yarn run test:lint --fix",
    "version": "npm-run-all test bundle && git add -A dist/",
    "tailwind": "npx tailwindcss -i ./src/app/styles/input.css -o ./src/app/styles/tailwind.css --config ./mainConfigs/tailwind.config.js --watch"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "classnames": "^2.3.2",
    "immutability-helper": "^3.1.1",
    "json-server": "^1.0.0-beta.2",
    "lodash": "^4.17.21",
    "postcss-preset-env": "^10.0.3",
    "prop-types": "^15.8.1",
    "rdndmb-html5-to-touch": "^8.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^8.0.0",
    "react-dnd-touch-backend": "^16.0.1",
    "react-redux": "^9.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^4.15.1",
    "@blueprintjs/icons": "^4.13.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/chai": "^4.3.0",
    "@types/classnames": "^2.3.1",
    "@types/dom4": "^2.0.2",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^7.0.2",
    "@types/node": "^22.5.5",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.6",
    "concurrently": "^9.0.1",
    "css-loader": "^6.11.0",
    "dnd-core": "16.0.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^25.0.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.13.1",
    "less-loader": "^11.1.0",
    "less-plugin-autoprefix": "^1.4.1",
    "mocha": "^10.7.3",
    "mocha-junit-reporter": "^1.23.3",
    "mock-require": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.11",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "yarn-deduplicate": "^6.0.1"
  },
  "peerDependencies": {
    "react": "16 - 18"
  }
}